{
  "presets": [
    "babel-preset-env",
    "react",
    "stage-2",
    [
      "env",
      {
        "targets": {
          "browsers": ["> 1%", "last 2 versions"]
        }
      }
    ]
  ],
  "compact": "false",
  "plugins": [
    "react-hot-loader/babel",
    "transform-class-properties",
    "syntax-dynamic-import"
  ]
}
